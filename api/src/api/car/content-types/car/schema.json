{
  "kind": "collectionType",
  "collectionName": "cars",
  "info": {
    "singularName": "car",
    "pluralName": "cars",
    "displayName": "Cars",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "pluginOptions": {}
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {}
    },
    "price": {
      "type": "integer",
      "required": true,
      "pluginOptions": {}
    },
    "counter": {
      "type": "integer",
      "required": true,
      "pluginOptions": {}
    },
    "class": {
      "type": "enumeration",
      "enum": [
        "econom",
        "comfort",
        "business",
        "premium",
        "suv",
        "minivan"
      ],
      "required": true,
      "default": "econom"
    },
    "mileage": {
      "type": "integer",
      "required": true
    },
    "engine_type": {
      "type": "enumeration",
      "enum": [
        "gasoline",
        "diesel",
        "hybrid",
        "electricity"
      ],
      "required": true,
      "default": "gasoline"
    },
    "engine_capacity": {
      "required": true,
      "type": "float"
    },
    "engine_power": {
      "required": true,
      "type": "integer"
    },
    "drive_unit": {
      "type": "enumeration",
      "enum": [
        "front_wheel",
        "rear"
      ],
      "required": true,
      "default": "front_wheel"
    },
    "transmission": {
      "type": "enumeration",
      "enum": [
        "automatic",
        "manual"
      ],
      "required": true,
      "default": "automatic"
    },
    "location": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::location.location"
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "description_ru": {
      "type": "richtext",
      "required": true
    },
    "description_ge": {
      "type": "richtext",
      "required": true
    },
    "hit": {
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
